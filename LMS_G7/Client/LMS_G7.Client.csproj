<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Components\**" />
    <Compile Remove="Component\**" />
    <Content Remove="Components\**" />
    <Content Remove="Component\**" />
    <EmbeddedResource Remove="Components\**" />
    <EmbeddedResource Remove="Component\**" />
    <None Remove="Components\**" />
    <None Remove="Component\**" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="7.0.13" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="7.0.13" PrivateAssets="all" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="7.0.13" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="7.0.0" />
    <PackageReference Include="Syncfusion.Blazor" Version="24.1.44" />
    <PackageReference Include="Syncfusion.Blazor.Buttons" Version="24.1.44" />
    <PackageReference Include="Syncfusion.Blazor.Popups" Version="24.1.44" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Shared\LMS_G7.Shared.csproj" />
  </ItemGroup>

</Project>
