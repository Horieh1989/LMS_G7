@ExtraCaption
<h3>List of courses </h3>
<a href="@($"CourseAdd")" class="btn btn-outline-primary">Add</a>@* reference page ro miarim ba id devive age bekhaim *@
<table class="table">
    <thead>
        <tr>
            <th scope="col">Course Id</th>
            <th scope="col">Course Name</th>
            <th scope="col">Course Start Date</th>
            <th scope="col">Course End Date</th>
            <th scope="col">Course Description</th>
            <th scope="col">View</th>
            <th scope="col">Delete</th>
            <th scope="col">Update</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var course in CourseLst)
        {
            <tr>

                <td scope="row">@course.Id</td>
                <td scope="row">@course.Name</td>
                <td scope="row">@course.StartDate</td>
                <td scope="row">@course.EndDate</td>
                <td scope="row">@course.Description </td>
                <td scope="row"><a href="@($"courseview/{course.Id}")" class="btn btn-outline-primary">View</a></td>
              
               <td scope="row"><a href="@($"coursedelete/{course.Id}")" class="btn btn-outline-primary">Delete</a></td>@* reference page ro miarim ba id devive age bekhaim *@
               <td scope="row"><a href="@($"courseupdate/{course.Id}")" class="btn btn-outline-primary">Update</a></td>@* reference page ro miarim ba id devive age bekhaim *@
               
            </tr>

        }
    </tbody>



</table>
